{
  "name": "Chrome Apps Launcher",
  "description": "Chrome Apps Launcher (by daiiz)",
  "manifest_version": 2,
  "version": "0.1.0",
  "browser_action": {
    "default_icon": {
      "19": "seed-icon.png",
      "38": "seed-icon.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "management",
    "<all_urls>"
  ],
  "icons": {
    "16": "seed-icon.png",
    "32": "seed-icon.png",
    "48": "seed-icon.png",
    "128": "seed-icon.png"
  }
}